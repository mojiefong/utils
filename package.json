{
  "name": "@mojiee/utils",
  "type": "module",
  "version": "0.7.0",
  "description": "Utility function",
  "author": "Mojie",
  "license": "MIT",
  "homepage": "https://github.com/jayfongs/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfongs/utils.git"
  },
  "bugs": {
    "url": "https://github.com/jayfongs/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "rimraf dist && npm run build",
    "prepare": "husky install",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "release": "npx bumpp --commit --push --tag --no-verify",
    "docs": "rimraf dist && typedoc --plugin typedoc-plugin-markdown",
    "docs:html": "rimraf dist && typedoc --out docs-html --plugin typedoc-plugin-missing-exports",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/node": "^22.13.4",
    "@types/throttle-debounce": "^5.0.2",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "rimraf": "^6.0.1",
    "throttle-debounce": "^5.0.2",
    "typedoc": "^0.27.7",
    "typedoc-plugin-markdown": "^4.4.2",
    "typedoc-plugin-missing-exports": "^3.1.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.6"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
